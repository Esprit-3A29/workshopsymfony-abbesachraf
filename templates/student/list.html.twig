{{ form(searchForm) }}
<a href="http://127.0.0.1:8000/AjouterStudent">Add Student</a>
<h4>List Student</h4>
{% for s in  tabStudent %}
    nce: {{ s.nce }}
    username: {{ s.username }}
    moyenne: {{ s.moyenne }}
    <a href="{{ path('ModifierStudent', {'id': s.nce}) }}">Update</a>
    <a href="{{ path('SupprimerStudent', {'id': s.nce}) }}">Remove</a>
    <br>
{% endfor %}

<h1 style="color: coral">List Students order By</h1>
{% for i in sortByMoyenne %}
nce: {{ i.nce }}
username: {{ i.username }}
moyenne: {{ i.moyenne }}
<br>
{% endfor %}
<h1 style="color: deeppink">Top Students</h1>
{% for i in topStudents %}
    nce: {{ i.nce }}
    username: {{ i.username }}
    moyenne: {{ i.moyenne }}
    <br>
{% endfor %}